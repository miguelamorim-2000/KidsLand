<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#46D0D9"
    app:layout_behavior="@string/appbar_scrolling_view_behavior">


    <EditText
        android:id="@+id/editTextTextEmailAddress8"
        android:layout_width="157dp"
        android:layout_height="32dp"
        android:layout_marginBottom="364dp"
        android:background="@drawable/textfieldround"
        android:ems="10"
        android:inputType="textEmailAddress"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintHorizontal_bias="0.858"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/emailtittle" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="419dp"
        android:layout_height="734dp"
        android:layout_marginTop="192dp"
        android:contentDescription="@android:string/untitled"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/vetor1" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="4dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/logo" />

    <TextView
        android:id="@+id/editTextTextPersonName"
        android:layout_width="210dp"
        android:layout_height="66dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="70dp"
        android:background="@null"
        android:ems="10"
        android:fontFamily="@font/rancho"
        android:inputType="textPersonName"
        android:text="Crie o seu Perfil !"
        android:textColor="#143656"
        android:textSize="38dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.524"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@id/imageView3"
        app:layout_constraintVertical_bias="0.343" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="37dp"
        android:layout_marginBottom="32dp"
        android:background="@drawable/buttonlogin"
        android:text="Criar"
        android:textStyle="bold"
        app:backgroundTint="@null"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        app:layout_constraintVertical_bias="1.0" />

    <EditText
        android:id="@+id/editTextTextEmailAddress5"
        android:layout_width="157dp"
        android:layout_height="32dp"
        android:layout_marginBottom="364dp"
        android:background="@drawable/textfieldround"
        android:ems="10"
        android:inputType="textEmailAddress"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintHorizontal_bias="0.858"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/emailtittle" />

    <EditText
        android:id="@+id/editTextTextEmailAddress9"
        android:layout_width="157dp"
        android:layout_height="32dp"
        android:layout_marginBottom="468dp"
        android:background="@drawable/textfieldround"
        android:ems="10"
        android:inputType="textEmailAddress"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintHorizontal_bias="0.862"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/emailtittle" />

    <EditText
        android:id="@+id/editTextTextEmailAddress10"
        android:layout_width="157dp"
        android:layout_height="32dp"
        android:layout_marginBottom="468dp"
        android:background="@drawable/textfieldround"
        android:ems="10"
        android:inputType="textEmailAddress"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintHorizontal_bias="0.137"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/emailtittle" />

    <EditText
        android:id="@+id/editTextTextEmailAddress7"
        android:layout_width="157dp"
        android:layout_height="32dp"
        android:layout_marginBottom="364dp"
        android:background="@drawable/textfieldround"
        android:ems="10"
        android:inputType="textEmailAddress"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintHorizontal_bias="0.137"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/emailtittle" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="129dp"
        android:layout_height="15dp"
        android:layout_marginBottom="408dp"
        android:fontFamily="@font/rubik"
        android:text="Password:"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textColor="#13497B"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintHorizontal_chainStyle="packed"
        tools:layout_editor_absoluteX="35dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="160dp"
        android:layout_height="16dp"
        android:layout_marginBottom="408dp"
        android:fontFamily="@font/rubik"
        android:text="Confirmar Password:"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textColor="#13497B"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintHorizontal_chainStyle="packed"
        tools:layout_editor_absoluteX="218dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="156dp"
        android:layout_height="14dp"
        android:layout_marginBottom="512dp"
        android:fontFamily="@font/rubik"
        android:text="Data de Nascimento:"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textColor="#13497B"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        tools:layout_editor_absoluteX="219dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="156dp"
        android:layout_height="14dp"
        android:layout_marginBottom="512dp"
        android:fontFamily="@font/rubik"
        android:text="Nome de Utilizador:"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textColor="#13497B"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        tools:layout_editor_absoluteX="36dp" />


</androidx.constraintlayout.widget.ConstraintLayout>
