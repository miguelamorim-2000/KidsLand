<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#46D0D9"
    tools:context=".EditProfilePicActivity">


    <Button
        android:id="@+id/buttonbackpic"
        android:layout_width="32dp"
        android:layout_height="32dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/ic_back"
        app:backgroundTint="@null"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="419dp"
        android:layout_height="734dp"
        android:layout_marginTop="164dp"
        android:contentDescription="@android:string/untitled"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/vetor1" />

    <Button
        android:id="@+id/buttonApply2"
        android:layout_width="110dp"
        android:layout_height="40dp"
        android:layout_marginTop="49dp"
        android:layout_marginBottom="44dp"
        android:background="@drawable/buttonlogin"
        android:text="Guardar"
        android:textStyle="bold"
        app:backgroundTint="@null"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rapaz2.png" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="4dp"
        android:contentDescription="@string/logo"
        app:layout_constraintBottom_toTopOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/logo" />

    <TextView
        android:id="@+id/editTextTextTitulo2"
        android:layout_width="194dp"
        android:layout_height="49dp"
        android:layout_marginTop="25dp"
        android:background="@null"
        android:ems="10"
        android:fontFamily="@font/rancho"
        android:gravity="center_horizontal"
        android:inputType="textPersonName"
        android:text="Editar Fotografia"
        android:textColor="#143656"
        android:textSize="38sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView8"
        tools:ignore="TextViewEdits" />

    <ImageView
        android:id="@+id/rapaz3.png"
        android:layout_width="97dp"
        android:layout_height="97dp"
        android:layout_marginStart="25dp"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="35dp"
        android:layout_marginBottom="132dp"
        android:background="@drawable/rapaz3"
        android:contentDescription="@string/icon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rapariga1.png" />

    <ImageView
        android:id="@+id/rapaz3round.png"
        android:layout_width="97dp"
        android:layout_height="97dp"
        android:layout_marginStart="25dp"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="35dp"
        android:layout_marginBottom="132dp"
        android:background="@drawable/round"
        android:contentDescription="@string/icon"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rapariga1round.png" />

    <ImageView
        android:id="@+id/rapaz2.png"
        android:layout_width="97dp"
        android:layout_height="97dp"
        android:layout_marginStart="25dp"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="35dp"
        android:layout_marginBottom="132dp"
        android:background="@drawable/rapaz2"
        android:contentDescription="@string/icon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/rapaz3.png"
        app:layout_constraintTop_toBottomOf="@+id/rapaz1.png"
        />
    <ImageView
        android:id="@+id/rapaz2round.png"
        android:layout_width="97dp"
        android:layout_height="97dp"
        android:layout_marginStart="25dp"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="35dp"
        android:layout_marginBottom="132dp"
        android:visibility="invisible"
        android:background="@drawable/round"
        android:contentDescription="@string/icon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/rapaz3round.png"
        app:layout_constraintTop_toBottomOf="@+id/rapaz1round.png"
        />

    <ImageView
        android:id="@+id/rapariga3.png"
        android:layout_width="97dp"
        android:layout_height="97dp"
        android:layout_marginStart="23dp"
        android:layout_marginLeft="23dp"
        android:layout_marginTop="35dp"
        android:layout_marginBottom="132dp"
        android:background="@drawable/rapariga3"
        android:contentDescription="@string/icon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/rapaz2.png"
        app:layout_constraintTop_toBottomOf="@+id/rapariga2.png" />

    <ImageView
        android:id="@+id/rapariga3round.png"
        android:layout_width="97dp"
        android:layout_height="97dp"
        android:layout_marginStart="23dp"
        android:visibility="invisible"
        android:layout_marginLeft="23dp"
        android:layout_marginTop="35dp"
        android:layout_marginBottom="132dp"
        android:background="@drawable/round"
        android:contentDescription="@string/icon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/rapaz2round.png"
        app:layout_constraintTop_toBottomOf="@+id/rapariga2round.png" />

    <ImageView
        android:id="@+id/rapariga1.png"
        android:layout_width="97dp"
        android:layout_height="97dp"
        android:layout_marginStart="25dp"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="106dp"
        android:background="@drawable/rapariga1"
        android:contentDescription="@string/icon"
        app:layout_constraintBottom_toTopOf="@+id/rapaz3.png"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextTitulo2"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:id="@+id/rapariga1round.png"
        android:layout_width="97dp"
        android:layout_height="97dp"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="106dp"
        android:background="@drawable/round"
        android:visibility="invisible"
        android:contentDescription="@string/icon"
        app:layout_constraintBottom_toTopOf="@+id/rapaz3round.png"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextTitulo2"
        app:layout_constraintVertical_bias="0.027" />

    <ImageView
        android:id="@+id/rapaz1.png"
        android:layout_width="97dp"
        android:layout_height="97dp"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="106dp"
        android:background="@drawable/rapaz1"
        android:contentDescription="@string/icon"
        app:layout_constraintBottom_toTopOf="@+id/rapaz2.png"
        app:layout_constraintStart_toEndOf="@+id/rapariga1.png"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextTitulo2" />


    <ImageView
        android:id="@+id/rapaz1round.png"
        android:layout_width="97dp"
        android:layout_height="97dp"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="106dp"
        android:background="@drawable/round"
        android:visibility="invisible"
        android:contentDescription="@string/icon"
        app:layout_constraintBottom_toTopOf="@+id/rapaz2round.png"
        app:layout_constraintStart_toEndOf="@+id/rapariga1round.png"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextTitulo2"
        app:layout_constraintVertical_bias="0.027" />

    <ImageView
        android:id="@+id/rapariga2.png"
        android:layout_width="97dp"
        android:layout_height="97dp"
        android:layout_marginStart="25dp"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="106dp"
        android:background="@drawable/rapariga2"
        android:contentDescription="@string/icon"
        app:layout_constraintBottom_toTopOf="@+id/rapariga3.png"
        app:layout_constraintStart_toEndOf="@+id/rapaz1.png"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextTitulo2"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:id="@+id/rapariga2round.png"
        android:layout_width="97dp"
        android:layout_height="97dp"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="106dp"
        android:background="@drawable/round"
        android:visibility="invisible"
        android:contentDescription="@string/icon"
        app:layout_constraintBottom_toTopOf="@+id/rapariga3round.png"
        app:layout_constraintStart_toEndOf="@+id/rapaz1round.png"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextTitulo2"
        app:layout_constraintVertical_bias="0.0" />

</androidx.constraintlayout.widget.ConstraintLayout>